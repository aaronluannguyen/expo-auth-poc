# WorkOS Configuration
# Get these values from your WorkOS Dashboard: https://dashboard.workos.com

# Your WorkOS Client ID (required)
# Example: client_01H5JXXXXXXXXXXXXXXXXXXX
EXPO_PUBLIC_WORKOS_CLIENT_ID=your_workos_client_id

# Your app's redirect URI (required)
# For development with Expo Go: use exp://localhost:8081
# For production: use your app's custom scheme or universal link
# Example: myapp://oauth/callback or https://yourapp.com/oauth/callback
EXPO_PUBLIC_WORKOS_REDIRECT_URI=exp://localhost:8081

# WorkOS Discovery URL (optional - defaults to WorkOS production)
EXPO_PUBLIC_WORKOS_DISCOVERY_URL=https://api.workos.com/.well-known/openid-configuration

# WorkOS Authorization Endpoint (optional - derived from discovery)
EXPO_PUBLIC_WORKOS_AUTH_ENDPOINT=https://api.workos.com/sso/authorize

# WorkOS Token Endpoint (optional - derived from discovery)
EXPO_PUBLIC_WORKOS_TOKEN_ENDPOINT=https://api.workos.com/sso/token

# WorkOS UserInfo Endpoint (optional)
EXPO_PUBLIC_WORKOS_USERINFO_ENDPOINT=https://api.workos.com/user_management/userinfo

# App Configuration
EXPO_PUBLIC_ENVIRONMENT=development
EXPO_PUBLIC_DEBUG=true
